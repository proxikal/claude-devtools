{
  "$schema": "https://unpkg.com/knip@next/schema.json",
  "entry": [
    "src/main/index.ts",
    "src/preload/index.ts",
    "src/renderer/main.tsx",
    "electron.vite.config.ts"
  ],
  "project": ["src/**/*.{ts,tsx}!"],
  "ignore": ["tsconfig*.json"],
  "paths": {
    "@main/*": ["./src/main/*"],
    "@renderer/*": ["./src/renderer/*"],
    "@preload/*": ["./src/preload/*"],
    "@shared/*": ["./src/shared/*"]
  }
}
